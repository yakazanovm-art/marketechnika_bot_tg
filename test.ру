print("=" * 50)
print("✅ Тестовый скрипт запущен!")
print("=" * 50)

# Проверяем импорты
try:
    from telegram import version as tg_version
    print(f"✅ python-telegram-bot версия: {tg_version}")
except ImportError as e:
    print(f"❌ Ошибка импорта telegram: {e}")

try:
    import flask
    print(f"✅ Flask версия: {flask.__version__}")
except ImportError as e:
    print(f"❌ Ошибка импорта flask: {e}")

print("✅ Скрипт завершился успешно!")
